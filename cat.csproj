<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="cat.mp3" />
    <None Remove="System.Windows.Extensions" />
    <None Remove="NAudio" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Windows.Extensions" Version="7.0.0" />
    <PackageReference Include="NAudio" Version="2.1.0" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="**/*" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="cat.sln" />
    <EmbeddedResource Remove="Program.cs" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Program.cs" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="cat.sln" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="cat.mp3">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>
</Project>
